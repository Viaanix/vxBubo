{
    "env": {
        "browser": true,
        "es2021": true,
        "es6": true,
        "jquery": true
    },
    "extends": [
        "standard",
        "eslint:recommended",
        "plugin:import/recommended"
    ],
    "plugins": [
        "import"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": [".bubo/**/*"],
    "rules": {
        "semi": [2, "always"]
    }
}
